(declare-rel Invariant (Bool Bool Bool))
(declare-rel Goal ())
(declare-var A Bool)
(declare-var B Bool)
(declare-var C Bool)
(declare-var D Bool)
(declare-var E Bool)
(declare-var F Bool)
(rule (=> (not (or A B C)) (Invariant A B C)))
(rule (=> (and (Invariant D E F)
				(=> (not (and D)) (not B))
				(=> (and D) B)
				(=> (not (and E)) (not C))
				(=> (and E) C)
	) (Invariant A B C)))
(rule (=> (and (Invariant A B C) A B C) Goal))
(query Goal)